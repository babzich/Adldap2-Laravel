{
    "name": "adldap2/adldap2-laravel",
    "description": "LDAP Authentication & Management for Laravel.",
    "keywords": ["adldap", "adldap2", "ldap", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/babzich/Adldap2"
        }
    ],
    "require": {
        "php": ">=5.6",
        "adldap2/adldap2": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.6",
        "orchestra/testbench": "~3.2"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Adldap\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adldap\\Laravel\\Tests\\": "tests/"
        }
    }
}
